@model FinalDemo.DTOs.TicketDTO

@{
    ViewBag.Title = "Ticket Details";
}

<h2>Ticket Details</h2>

<div class="well">
    <dl class="dl-horizontal">
        <dt>Ticket ID</dt>
        <dd>@Model.TicketId</dd>

        <dt>Issue</dt>
        <dd>@Model.Issue</dd>

        <dt>Status</dt>
        <dd>@Model.Status</dd>
    </dl>

    <a href="@Url.Action("Index", "Tickets")" class="btn btn-primary">Back to List</a>
</div>
